{
  "name": "express-tdd",
  "version": "1.0.0",
  "description": "this app is for TDD learning purpose",
  "main": "src/app.js",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "test": "jest --watch"
  },
  "author": "Sergio Velasquez",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.1",
    "sequelize": "^6.21.2",
    "sqlite3": "^5.0.8"
  },
  "devDependencies": {
    "eslint": "^8.18.0",
    "jest": "^28.1.1",
    "nodemon": "^2.0.18",
    "supertest": "^6.2.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    },
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "semi": "warn",
      "quotes": [
        "warn",
        "singles"
      ]
    }
  }
}
